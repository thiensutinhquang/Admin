{
  "name": "Sự kiện",
  "short_name": "Sự kiện",
  "description": "Ứng dụng quản lý sự kiện",
  "start_url": "./",
  "scope": "./",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "tabbed"],
  "orientation": "portrait",
  "background_color": "#1e293b",
  "theme_color": "#1e293b",
  "id": "./",
  "icons": [
    {
      "src": "icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "icon-192-any.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "images/screenshot1.png",
      "type": "image/png",
      "sizes": "1280x720",
      "form_factor": "wide",
      "label": "Giao diện chính trên máy tính bảng"
    },
    {
      "src": "images/screenshot2.png",
      "type": "image/png",
      "sizes": "720x1280",
      "form_factor": "narrow",
      "label": "Giao diện chính trên điện thoại"
    },
    {
      "src": "images/screenshot3.png",
      "type": "image/png",
      "sizes": "1280x720",
      "form_factor": "wide",
      "label": "Tính năng thêm sự kiện"
    }
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      },
      "icons": [
        {
          "src": "icon-48.png",
          "sizes": "48x48",
          "type": "image/png"
        }
      ]
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+sukien",
      "url": "/handle-protocol?url=%s"
    }
  ]
}
